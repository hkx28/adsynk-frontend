# Adsynk Frontend Environment Variables

# API Base URL - Replace with your actual API Gateway URL
REACT_APP_API_BASE_URL=https://your-api-gateway-url.amazonaws.com/prod

# MediaLive API Configuration
REACT_APP_MEDIALIVE_REGION=ap-northeast-2

# Development/Production Mode
REACT_APP_ENV=development

# Enable/Disable Features
REACT_APP_ENABLE_MEDIALIVE_INTEGRATION=true
REACT_APP_ENABLE_DEBUG_MODE=false

# Timeout Settings (milliseconds)
REACT_APP_API_TIMEOUT=10000
REACT_APP_MEDIALIVE_TIMEOUT=5000 